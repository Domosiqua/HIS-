            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="deptmentId" column="deptment_id" jdbcType="INTEGER"/>
            <result property="registLevelId" column="regist_level_id" jdbcType="INTEGER"/>
            <result property="schedulingId" column="scheduling_id" jdbcType="INTEGER"/>
            <result property="delmark" column="delmark" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,realname,password,
        deptment_id,regist_level_id,scheduling_id,
        delmark
    </sql>
</mapper>
